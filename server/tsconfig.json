{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es5",
        "noImplicitAny": true,
        "moduleResolution": "node",
        "sourceMap": true,
        "outDir": "dist",  // TS文件编译后会放入到此文件夹内
        "baseUrl": ".",
        "paths": {
            "*": [
                "node_modules/*",  //根据项目的路径指定
                "*"      //根据项目的路径指定
            ]
        }
    },
    "include": [
        "*"                //根据项目的路径指定
    ]
}
